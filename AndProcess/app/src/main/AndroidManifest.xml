<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="sbl.com.andprocess"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".HandlerActivity"
            android:icon="@drawable/ic_globe">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

            </intent-filter>
        </activity>

        <activity android:name=".AsyncTaskActivity"

            />

        <activity android:name=".IntentServiceActivity"/>
        <activity android:name=".ServiceActivity"/>
        <activity android:name=".ThreadActivity"/>
        <activity android:name=".RotateActivity"/>


        <!--Service declared in manifest -->
        <service android:name="sbl.com.andprocess.services.SampleService"
                 android:exported="false"/>

        <service android:name="sbl.com.andprocess.services.SampleServiceSecond"
            android:exported="false"/>


    </application>

</manifest>
